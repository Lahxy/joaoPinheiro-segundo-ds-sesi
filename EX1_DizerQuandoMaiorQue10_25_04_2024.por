//EX1-Faça um algoritmo que receba um número e mostre uma mensagem caso este número seja maior que 10.

programa {
  funcao inicio()
  {
    inteiro n
    enquanto (verdadeiro)
    {
      escreva ("digite um número: ")
      leia (n)
      se (n>10)
      {
        escreva ("<<<VOCÊ DIGITOU UM NÚMERO MAIOR QUE 10>>>\n\n")
      }
    }
  }
}
